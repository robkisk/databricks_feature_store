    {
      "name": "Python",
      "type": "python",
      "stopOnEntry": false,
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
